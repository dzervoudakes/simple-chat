  "description": "Simple Chat frontend, built with React, TypeScript and Socket.io-client.",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production ts-node scripts/build",
    "build:report": "npm run build -- --report",
    "clean": "rimraf coverage dist docs",
    "docs": "typedoc",
    "lint": "node -r ts-node/register scripts/eslint",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "stylelint": "ts-node scripts/stylelint",
    "stylelint:fix": "npm run stylelint -- --fix",
    "test": "ts-node scripts/test",
    "test:coverage": "npm test -- --coverage",
    "test:verbose": "npm test -- --verbose",
    "test:watch": "npm test -- --watchAll"
  },
  "repository": {
